  "devDependencies": {
    "@babel/core": "7.19.1",
    "@babel/eslint-parser": "7.19.1",
    "@cloudflare/workers-types": "3.16.0",
    "@esm-bundle/chai": "4.3.4-fix.0",
    "@semantic-release/changelog": "6.0.1",
